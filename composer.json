{
    "name": "senna/utils",
    "description": "Shared for Senna",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Lennard",
            "email": "packagist@spring.nl"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Senna\\Utils\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Senna\\Utils\\Tests\\": "tests"
        }
    },
    "require-dev": {
         "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.5"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Senna\\Utils\\UtilsServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --testdox",
        "test-f": "vendor/bin/phpunit --filter"
    }
}
